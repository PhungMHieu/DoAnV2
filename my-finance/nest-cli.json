{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/transaction-service/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/transaction-service/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/transaction-service",
  "projects": {
    "auth-common": {
      "type": "library",
      "root": "libs/auth-common",
      "entryFile": "index",
      "sourceRoot": "libs/auth-common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth-common/tsconfig.lib.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "group-service": {
      "type": "application",
      "root": "apps/group-service",
      "entryFile": "main",
      "sourceRoot": "apps/group-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/group-service/tsconfig.app.json"
      }
    },
    "ml-service": {
      "type": "application",
      "root": "apps/ml-service",
      "entryFile": "main",
      "sourceRoot": "apps/ml-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ml-service/tsconfig.app.json",
        "assets": [
          {
            "include": "grpc/**/*.proto",
            "watchAssets": true
          }
        ]
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/common/redis",
      "entryFile": "index",
      "sourceRoot": "libs/common/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/redis/tsconfig.lib.json"
      }
    },
    "redis-common": {
      "type": "library",
      "root": "libs/redis-common",
      "entryFile": "index",
      "sourceRoot": "libs/redis-common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis-common/tsconfig.lib.json"
      }
    },
    "report-service": {
      "type": "application",
      "root": "apps/report-service",
      "entryFile": "main",
      "sourceRoot": "apps/report-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/report-service/tsconfig.app.json"
      }
    },
    "rmq-common": {
      "type": "library",
      "root": "libs/rmq-common",
      "entryFile": "index",
      "sourceRoot": "libs/rmq-common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rmq-common/tsconfig.lib.json"
      }
    },
    "swagger": {
      "type": "library",
      "root": "libs/swagger",
      "entryFile": "index",
      "sourceRoot": "libs/swagger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/swagger/tsconfig.lib.json"
      }
    },
    "websocket-common": {
      "type": "library",
      "root": "libs/websocket-common",
      "entryFile": "index",
      "sourceRoot": "libs/websocket-common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/websocket-common/tsconfig.lib.json"
      }
    },
    "transaction-service": {
      "type": "application",
      "root": "apps/transaction-service",
      "entryFile": "main",
      "sourceRoot": "apps/transaction-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/transaction-service/tsconfig.app.json"
      }
    }
  }
}