{
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "resolvePackageJsonExports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/auth-common": [
        "libs/auth-common/src"
      ],
      "@app/auth-common/*": [
        "libs/auth-common/src/*"
      ],
      "@app/common": [
        "libs/common/src"
      ],
      "@app/common/*": [
        "libs/common/src/*"
      ],
      "@app/redis": [
        "libs/common/redis/src"
      ],
      "@app/redis-common": [
        "libs/redis-common/src"
      ],
      "@app/redis-common/*": [
        "libs/redis-common/src/*"
      ],
      "@app/redis/*": [
        "libs/common/redis/src/*"
      ],
      "@app/rmq-common": [
        "libs/rmq-common/src"
      ],
      "@app/rmq-common/*": [
        "libs/rmq-common/src/*"
      ],
      "@app/swagger": [
        "libs/swagger/src"
      ],
      "@app/swagger/*": [
        "libs/swagger/src/*"
      ]
    }
  }
}